a=int(input("Enter a number:"))
b=int(input("Enter a number:"))
c=int(input("Enter a number:"))
if(a==b and a>c):
  print("a and b are greater and equal")
elif(a==c and a>b):
  print("a and b are greater and equal")
 elif(b==c and b>a):
  print("b and c are greater and equal")
if(a>b and a>c):
  print(a,"is greater")
elif(b>c):
  print(b,"is greater")
else:
  print(c,"is greater")
